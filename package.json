{
  "name": "rspack-plugin-splash-screen",
  "version": "1.0.0",
  "description": "Rspack plugin for adding a splash screen to your app",
  "keywords": [
    "rspack",
    "rspack-plugin",
    "rsbuild",
    "splash-screen"
  ],
  "bugs": {
    "url": "https://github.com/arthurfiorette/rspack-plugin-splash-screen/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arthurfiorette/rspack-plugin-splash-screen.git"
  },
  "funding": "https://github.com/arthurfiorette/rspack-plugin-splash-screen?sponsor=1",
  "license": "MIT",
  "author": "Arthur Fiorette <npm@arthur.place>",
  "exports": {
    ".": {
      "require": "./dist/plugin/plugin.js",
      "import": "./dist/plugin/plugin.mjs",
      "types": "./dist/plugin/plugin.d.ts"
    },
    "./runtime": {
      "require": "./dist/runtime/runtime.js",
      "import": "./dist/runtime/runtime.mjs",
      "types": "./dist/runtime/runtime.d.ts"
    }
  },
  "main": "./dist/plugin/plugin.js",
  "types": "./dist/plugin/plugin.d.ts",
  "scripts": {
    "build": "rslib build",
    "build-all": "rslib build && pnpm -F example run build",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint-ci": "biome ci .",
    "lint-fix": "biome check --write --unsafe .",
    "prepare": "husky",
    "prepare-playwright": "playwright install --with-deps",
    "test": "playwright test",
    "test-types": "tsc --noEmit",
    "test-ui": "playwright test --ui"
  },
  "devDependencies": {
    "@arthurfiorette/biomejs-config": "catalog:",
    "@biomejs/biome": "catalog:",
    "@microsoft/api-extractor": "catalog:",
    "@playwright/test": "catalog:",
    "@rslib/core": "catalog:",
    "@rspack/core": "catalog:",
    "@types/node": "catalog:",
    "husky": "catalog:",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.28.2"
}
